/*CTQ*/
.basis{position:absolute; right:3px; top:0;} .basis dl:after{display:block; clear:both; content:"";}
.basis dt, .basis dd{float:left; padding:5px 15px 7px; height:38px; line-height:26px; box-sizing:border-box; border-radius:5px; box-shadow:2px 2px 2px rgba(0,0,0,.1); }
.basis dt{background-color:#a2aeb9; color:#fff;} .basis dd{margin-left:5px; background-color:#fff; border:1px solid #ddd;} .basis dd label{margin-right:15px; font-weight:800;}
.basis .pop{position:absolute; top:45px; right:0; padding:10px; border:1px solid #ddd; box-shadow:2px 2px 2px rgba(0,0,0,.1);}
.basis .pop label{float:left; display:block; padding:5px; background-color:#62a8d0; border-radius:3px; text-align:center; color:#fff; font-size:1.2em;}
.basis .pop label + label{margin-left:5px;}
.basis .pop input{display:block; margin-top:5px; width:70px; height:30px; padding:0 5px; background-color:#fff; border-color:#fff; text-align:center; font-weight:900; font-size:1.5em;}
.basis .pop p{clear:both; padding-top:10px; text-align:right;}

/*투입계획예측*/
.par.num .selected{float:left;} .par.num .selected > li{float:left; display:inline-block; margin-left:5px; padding:0 9px; line-height:26px; background-color:#65C7DB; border-radius:3px; color:#fff;}
.num .select{clear:both;} .par.num .select:after{display:block; clear:both; content:"";}
.num .select ul{float:left; margin-top:5px; width:20%;} .par.num .select li{margin:5px 0 0 15px; line-height:35px;}  .par.num .select ul:first-child li{margin-left:0;}
.num .select [type="checkbox"]{position:absolute; width:0; heght:0;} 
.num .select label{display:block; background-color:#eee; border:1px solid #ddd; border-radius:2px; text-align:center; font-size:1.2em; font-weight:600; cursor:pointer;}
.num .select :checked + label{background-color:#e3f4fa; border-color:#65C7DB; color:#2a94b8;}
.num .select :checked + label:before{margin:0 20px 0 -10px; content: "\f00c"; font-family: 'FontAwesome';}
.num + p{margin-top:10px; text-align:center;} .num + p .searchbn{padding:7px 30px; font-size:1.3em; line-height:1.6em;}
.titwrap input[type="number"]{border-color:#96b3bd; font-size:1.1em; font-weight:600;} .titwrap label + input{margin-left:10px;} 
.titwrap label{margin:0 4px; font-size:1.1em; color:#487c8e; font-weight:600;}







